# Environment variables for Vercel backend deployment
# Set these in Vercel dashboard for production

# MongoDB Connection
MONGODB_URI=mongodb+srv://your_username:your_password@cluster.mongodb.net/dhanraksha?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# Node Environment
NODE_ENV=production

# CORS Origins (will be updated with your actual frontend URL)
FRONTEND_URL=https://dhanraksha.vercel.app